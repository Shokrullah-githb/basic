{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 19,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "default",
      "displayName": "Default MinGW",
      "generator": "MinGW Makefiles",
      "binaryDir": "${sourceDir}/build",
      "cacheVariables": {
        "CMAKE_C_COMPILER": "C:/MinGW/bin/gcc.exe",
        "CMAKE_CXX_COMPILER": "C:/MinGW/bin/g++.exe"
      }
    }
  ]
}
